<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ParallelSuite" parallel="tests">
<test thread-count="5" name="Testing On Edge">
		<parameter name="browser" value="Edge"></parameter>
		<classes>
			<class name="GoogleSearch_Testing.Google_Search" />
		</classes>
	</test> <!--
	Test -->

	<test thread-count="5" name="Testing On Firefox">
		<parameter name="browser" value="Firefox"></parameter>
		<classes>
			<class name="GoogleSearch_Testing.Google_Search" />
		</classes>
	</test> <!--
	Test -->

	<test thread-count="5" name="Testing On Chrome">
		<parameter name="browser" value="Chrome"></parameter>
		<classes>
			<class name="GoogleSearch_Testing.Google_Search" />
		</classes>
	</test> <!--
	Test -->

</suite> <!--
Suite -->